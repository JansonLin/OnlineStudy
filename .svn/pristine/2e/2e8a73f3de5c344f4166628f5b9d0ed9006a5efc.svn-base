/* CSS Document */

*{
		padding:0;
		margin:0;
	}
	.nav{
		width:100%;
		height:80px;
		background:#000000;
	}
	.nav-as{
		margin-left:80px;
	}
	.nav-as li{
		list-style:none;
		display:inline-block;
		height:60px;
		line-height:80px;
		text-align:center;
		vertical-align:middle;
	}
	.nav-as li a{
		display:inline-block;
		text-decoration:none;
		margin-left:20px;
		font-family:"STLiti";
		font-size:30px;
		color:#CCCCCC;
	}
	.nav-as li a:hover{
		background-color:#FFFFFF;
		opacity:0.5;
		color:#000000;
	}
	.nav-cs,.nav-cs a{
		list-style:none;
		display:inline-block;
		float:right;
		margin-right:30px;
		line-height:80px;
	}
	.nav-cs{
		margin-left:240px;
		float:right;
	
	}
	.txt{
		margin-left:400px;
		height:30px;
		width:280px;
		border-radius:10px;
		}
	#btn{
		height:30px;
		width:30px;
		margin-left:-40px;
		margin-bottom:-10px;
	}
	
	#btn:hover input{
		border:outset #00FF00 1px solid;
		border-radious:5px;
	}
	
	.container{
		
		width:100%;
	}
	.con{
		list-style:none;
		height:200px;
		
		}
	.con li{
		padding:10px;
		line-height:40px;
		border-bottom:solid 1px #CCCCCC;
	}
	.con a{
	margin-left:25px;
	text-decoration:none;
	color:#000000;
	}
	#cd{
		display:inline-block;
		width:80px;
		height:30px;
		margin-bottom:0px;
		margin-left:0px;
		border:none;
		color:#000000;
	  }
	.jp{
		margin-left:80px;
		height:40px;
		}
	#cd:hover{
		color:red;
		cursor:pointer;
	}
	
	.con1{
		margin-top:30px;
 	}
	p{background-color:#ffc628;}
	.con1 li,.con2 li{
		display:inline-block;
		height:160px;
		width:280px;
		list-style:none;
		margin-bottom:120px;
		border-radius:10px;
		margin-left:60px;
		}
		.con2{
			margin-top:30px;
		}
	  .con2 li{
	 	 margin-bottom:30px;	  
		 }
	  .con2 p{
		  display:block;
		  margin-bottom:30px;
	  }
	  
	.con1 li:hover,.con2 li:hover {
		box-shadow:10px 10px 25px #ccc;
		cursor:pointer;
		color:red;
	}
	
	.ce{
	
		float:left;
	}
	.con1 img,.con2 img,.cr img{
		float:right;
		height:160px;
		width:280px;
		border:#BBBBFF outset 2px;
		border-radius:10%; 
		margin-bottom:10px;
	}
		.cr img{
		left:20px;
		height:100px;
		width:240px;
		background-color:#666666;
		opacity:0.7;
		border-radius:0;
	}
	#div1{
		float:left;
	}
	#div1 div{
		display:none;
		height:300px;
		width:240px;
		margin-left:20px;
	}
	.silde{
		height:300px;
		width:240px;
		margin-top:0;
		margin-left:20px;
	}
	.sla{
		display:inline-block;
		float:left;
		list-style:none;
		display:inline-block;
		margin-left:20px;
		
	}
	.sla li{
		display:block;
		height:100px;
		line-height:110px;
		width:240px;
		border:1px solid #CCCCCC;
		perspective: 1000px;
		position:relative;
		background-color:#FF0000;
	}
	.cr{
		display:none;
		height:100px;
		width:200px;
		text-align:center;
		position:absolute;
		transition:0.3s;	
		
	}
	
	.sla li span{
		display:block;
		height:100px;
		width:240px;
		text-align:center;
		position:absolute;
	}
	.sla li:hover{
		position:relative;
		border:hidden;
		background-color:#FFFFFF;
		z-index:2;
		transform:scale(1.1);
		}
	.sla li:hover .cr{
		display:block;
		text-align:center;
		cursor:pointer;
		transform: scaleZ(1.3) scaleY(1.4) scaleX(1.1);
	}
	.footer{
		display:block;
		height:60px;
		width:100%;
		text-align:center;
		background-color:#333333;	
	}
	.footer a{
		margin-left:60px;
		vertical-align:middle;
		text-align:center;
		text-decoration:none;
		color:#FFFFFF;
	}
	.cf{
		display:block;
		text-align:center;
		margin-top:40px;
	}
	
		.cf li{
		display:inline-block;
		height:60px;
		list-style:none;
		margin-bottom:120px;
		border-radius:10px;
		margin-left:40px;
		text-align:center;
		position:relative;
	
		
	}
	.cf li a{
		display:block;
		
		height:28px;
		width:28px;
		line-height:28px;
		text-align:center;
		vertical-align:middle;
		text-decoration:none;
		position:absolute;
	}
	.asd{
		display:none;
		height:28px;
		width:28px;
		border-radius:50%;
		background-color:#666666;
		opacity:0.5;
		cursor:pointer;
		position:absolute;
	}
	/*.cf li:hover .asd{
		display:block;
	}*/
	.con2 h3{
		font-family:"¡• È";
		font-size:36px;
		color:#33FFFF;
		text-align:center;
	}
		#c1 .active{
		background-color:#FF0000;
		
	}
	#div1 .active{
		background:#660000;
	}
	#logo.logo{
	float:left;
	height:74px;
	width:126px;
	margin-left:10px;}