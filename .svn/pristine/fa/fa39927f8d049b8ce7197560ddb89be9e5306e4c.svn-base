@CHARSET "UTF-8";

	*{
	margin:0;
	padding:0;
	}
	.table1{
	    margin-top:50px;
	    float:left;
		margin:0 auto;
		border-collapse:collapse;
		border:1px solid #666666;
		border-radius:10px;
		text-align:center;
		font-family:"宋体";
		font-size:12px;
	}
	.thead1{
		background-color:#A2D0FF;
		border-collapse:collapse;
		}
	.thead1 tr{
		height:40px;
	}
	.thead1 th{
		width:110px;
		border:8px outset #DDEEFF;
		
	}
	.thead1 th.th1{
		width:70px;
		
	}
	.thead1 th.th2{
		width:180px;
		
	}
	.tbody1{
		background-color:#C6E2FF;
	}
	.tbody1 tr{
		height:40px;
	}
	.tbody1 td{
		width:110px;
		border:5px inset #DDEEFF;
	}
	.tbody1 td.td1{
		width:70px;	
	}
	.tbody1 td.td2{
		width:180px;
	}
	
	td.th1{
		width:30px;
	}
	
	.searchPanel{
			appearance:none;
			margin:10px;
		}
	
	.searchPanel select{
		margin-bottom:10px;
		margin-left:30px;
		background-color:#C6E2FF;
		border:none;
		color:#000000;
		border:2px solid #C8C8C8;
		border-radius:8px 8px 8px 8px;
		height:30px;
		width:140px;
		}
		
	.searchPanel option{
		appearance:none;
		display:block;
		height:30px;
		background-color:transparent;
		color:#000000;
		border:none;
		line-height:40px;
		}
		.inputEle{
		color:#000000;
			margin-top:10px;
			margin-left:60px;
			height:30px;
			width:180px;
			background-color:#C6E2FF;
					}
		.btn{
			margin-top:10px;
			margin-left:-6px;
			border:none;
			width:60px;
			height:33px;
			background-color:#f47c20;
			
		}
		
		
		.icon-font{
		color:#33FF99;
		font-family:"华文琥珀";
		font-size:24px;
		}
		.nav{
    font-family:"LiSu";
	font-size:24px;
	color:#666;
	height:43px;
	line-height:43px;
	width:98%;
	background-color:#f2f2f2;
	display:block;
}
    .nav span{
	padding-left:15px;
	
}
