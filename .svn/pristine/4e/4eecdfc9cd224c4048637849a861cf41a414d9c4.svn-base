@CHARSET "UTF-8";

	*{
		margin:0;
		padding:0;
	}
	#content{
		background-color:#99CC66;
		margin:10px 60px;
	}
	.form1{
		display:block;
		border-bottom:10px solid #990000;
		border-left:10px solid #990000;
		border-right:10px solid #990000;
		border-top:3px solid #990000;
		box-shadow:10px 3px 10px #990000;
	}
	.table2{
		display:block;
		width:600px;
		margin-left:30px;
		padding:40px;
		vertical-align:middle;
	}
	.table2 tr{
		height:40px;
		font-family:"宋体";
		font-size:16px;
		vertical-align:middle;
	}
	.table2 th{
		display:inline-block;
		height:36px;
		line-height:40px;
		vertical-align:middle;
		width:180px;
	}
	.table2 td{
		text-align:center;
		vertical-align:middle; 
		width:200px;
	}
	.inputEle{
		height:40px;
		width:260px;
		background:#BCBC78;
	}
	img{
		height:60px;
		height:100px;
	}
	.btn{
		display:block;
		width:80px;
		height:40px;
		float:right;
		margin-top:30px;
		background:#f47c20;
	}
	
	.nav{
		margin-left:10px;
		margin-top:10px;
	}
		.icon-font{
			color:#33FF99;
			font-family:"华文琥珀";
			font-size:24px;
	}
		.nav span{
			left:30px;
			font-family:"华文隶书";
			font-size:22px;
	}
		.nav span a{
			text-decoration:none;
			font-family:"华文隶书";
			font-size:22px;
			color:#009900;
		}