package entity;

public class Course {
	private int CID;
	private String CName;
	private String CAddress;
	private String CDirection;
	private String CSort;
	private String CType;
	private String CImgAddress;
	private String CClass;
	
	public int getCID() {
		return CID;
	}
	public void setCID(int cID) {
		CID = cID;
	}
	public String getCName() {
		return CName;
	}
	public void setCName(String cName) {
		CName = cName;
	}
	public String getCAddress() {
		return CAddress;
	}
	public void setCAddress(String cAddress) {
		CAddress = cAddress;
	}
	public String getCDirection() {
		return CDirection;
	}
	public void setCDirection(String cDirection) {
		CDirection = cDirection;
	}
	public String getCSort() {
		return CSort;
	}
	public void setCSort(String cSort) {
		CSort = cSort;
	}
	public String getCType() {
		return CType;
	}
	public void setCType(String cType) {
		CType = cType;
	}
	public String getCImgAddress() {
		return CImgAddress;
	}
	public void setCImgAddress(String cImgAddress) {
		CImgAddress = cImgAddress;
	}
	public String getCClass() {
		return CClass;
	}
	public void setCClass(String cClass) {
		CClass = cClass;
	}
	
}
