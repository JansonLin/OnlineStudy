package entity;

public class Student {
	private int SID;
	private String SStudentID;
	private String SPwd;
	private String SName;
	private String SSex;
	private String SDepartment;
	private String SClass;
	private String SIntroduction;
	private String SEmail;
	public int getSID() {
		return SID;
	}
	public void setSID(int sID) {
		SID = sID;
	}
	public String getSStudentID() {
		return SStudentID;
	}
	public void setSStudentID(String sStudentID) {
		SStudentID = sStudentID;
	}
	public String getSPwd() {
		return SPwd;
	}
	public void setSPwd(String sPwd) {
		SPwd = sPwd;
	}
	public String getSName() {
		return SName;
	}
	public void setSName(String sName) {
		SName = sName;
	}
	public String getSSex() {
		return SSex;
	}
	public void setSSex(String sSex) {
		SSex = sSex;
	}
	public String getSDepartment() {
		return SDepartment;
	}
	public void setSDepartment(String sDepartment) {
		SDepartment = sDepartment;
	}
	public String getSClass() {
		return SClass;
	}
	public void setSClass(String sClass) {
		SClass = sClass;
	}
	public String getSIntroduction() {
		return SIntroduction;
	}
	public void setSIntroduction(String sIntroduction) {
		SIntroduction = sIntroduction;
	}
	public String getSEmail() {
		return SEmail;
	}
	public void setSEmail(String sEmail) {
		SEmail = sEmail;
	}
	
}
