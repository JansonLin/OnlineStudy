package entity;

public class Student {
	private int SID;				// 编号――自动获取
	private String SStudentID;		// 学号
	private String SPwd;			// 密码
	private String SName;			// 姓名
	private String SSex;			// 性别
	private String SDepartment;		// 系别
	private String SClass;			// 班级
	private String SIntroduction;	// 个人介绍
	private String SEmail;			// 电子邮件
	private String SImg;			// 头像
	private String SContactWay;		// 联系方式
	
	public int getSID() {
		return SID;
	}
	public void setSID(int sID) {
		SID = sID;
	}
	public String getSStudentID() {
		return SStudentID;
	}
	public void setSStudentID(String sStudentID) {
		SStudentID = sStudentID;
	}
	public String getSPwd() {
		return SPwd;
	}
	public void setSPwd(String sPwd) {
		SPwd = sPwd;
	}
	public String getSName() {
		return SName;
	}
	public void setSName(String sName) {
		SName = sName;
	}
	public String getSSex() {
		return SSex;
	}
	public void setSSex(String sSex) {
		SSex = sSex;
	}
	public String getSDepartment() {
		return SDepartment;
	}
	public void setSDepartment(String sDepartment) {
		SDepartment = sDepartment;
	}
	public String getSClass() {
		return SClass;
	}
	public void setSClass(String sClass) {
		SClass = sClass;
	}
	public String getSIntroduction() {
		return SIntroduction;
	}
	public void setSIntroduction(String sIntroduction) {
		SIntroduction = sIntroduction;
	}
	public String getSEmail() {
		return SEmail;
	}
	public void setSEmail(String sEmail) {
		SEmail = sEmail;
	}
	public String getSImg() {
		return SImg;
	}
	public void setSImg(String sImg) {
		SImg = sImg;
	}
	public String getSContactWay() {
		return SContactWay;
	}
	public void setSContactWay(String sContactWay) {
		SContactWay = sContactWay;
	}
	
}
