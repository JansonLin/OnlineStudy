@CHARSET "UTF-8";

	*{
	margin:0;
	padding:0;
	}

	.table1{
		margin:0 auto;
		border-collapse:collapse;
		border:1px solid #666666;
		border-radius:10px;
		text-align:center;
		font-family:"宋体";
		font-size:12px;
	}
	.thead1{
		background-color:#E1FFFF;
		border-collapse:collapse;
		}
	.thead1 tr{
		height:40px;
	}
	.thead1 th{
		width:110px;
		border:8px outset #CCCCCC;
		
	}
	.tbody1{
		background-color:#99CC99;
	}
	.tbody1 tr{
		height:40px;
	}
	.tbody1 td{
		width:110px;
		border:5px inset #CCCCCC;
	}
	
	td.th1{
		width:30px;
	}
	
	.searchPanel{
			appearance:none;
			margin:10px;
		}
	
	.searchPanel select{
		margin-bottom:10px;
		margin-left:30px;
		background-color:#999999;
		border:none;
		color:#FFFFFF;
		border-radius:10px 10px 0 0;
		height:30px;
		width:140px;
		}
		
	.searchPanel option{
		appearance:none;
		display:block;
		height:30px;
		background-color:transparent;
		color:#FFF;
		border:none;
		line-height:40px;
		}
