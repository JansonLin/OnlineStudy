@CHARSET "UTF-8";

 
	*{
		padding: 0;
		margin:0; 
	}
	#top_frame{
		width: 100%;
		height: 80px;
		background-color: #000;

	}
	#log{
		padding-top: 20px;
		color: #fff;
		font-size: 32px;
		font-family: "隶书";
		float: left;
		margin-left: 20px;

	}
	#main_menu{
		float: right;
		margin-right: 20px;
	}
	#main_menu ul{
		
	}
	#main_menu li{
		float: left;
		list-style:none;
		margin-left: 10px;
	}
	#main_menu a{
		padding-top: 20px;
		display: inline-block;
		height: 60px;
		font-size: 32px;
		font-family: "隶书";
		color: #fff;
		text-decoration:none;  
	}
	#main_menu a:hover{
		 color: #ee0;
		 background-color: #333;
	}
	#left_frame{
		font-family:"隶书";
		padding-top:5px;
		background-color: #f2f2f2;
		width: 12%;
		float: left;
		min-height: 400px;
		color:#666;
		font-size:18px;
		text-align:center;
		border-right: 1px solid #e5e5e5;
		overflow:hidden;
	}
	#main_frame{
		background-color: #fff;
		float: right;
		min-height:600px;
		min-width:1000px;
		margin-top:10px;
		border:1px solid #f2f2f2;
		width:87%;
	}
	#left_frame ul{
		width: 100%;
	}
	#left_frame li{
		width: 100%;
		list-style-type:none;
		border-bottom: 1px solid #e5e5e5;
		border-width:1px 0;
		padding-left:10px;
		text-align: left;
	}
	#left_frame a{
		padding-left:0 16 0 20;
		width:100%;
		display:block;
		height: 44px;
		line-height:44px;
		color:#666;
		text-decoration: none;
	}
	#second1{
		background-color: #fff;

	}
	#second2{
		background-color: #fff;

	}
	#second3{
		background-color: #fff;

	}
	#second4{
		background-color: #fff;

	}
	#second1 a,#second2 a,#second3 a{
		padding-left:18px;
	}
	#left_frame .clearBorder{
		border:0px;
	}
	.icon-font{
			font-size:14px;
			margin-right:3px;
	}
}

.icon-font {
	font-family: 'icomoon';
	speak: none;
	font-weight: 400;
	font-style: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
}
.content_container{
			min-height:600px;
			min-width:1000px;
}
