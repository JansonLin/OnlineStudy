	var aDiv=oDiv.getElementsByTagName("div");
			
	
		for(var i=0;i<aBtn.length;i++){
			 aBtn[i].index=i;
		
			aBtn[i].onclick=function(){
			
				//	 alert('你好');
				for(var i=0;i<aBtn.length;i++){
					aBtn[i].className='';
					aDiv[i].style.display='';
				}
					this.className='active';
					aDiv[this.index].style.display='block';
			};
			
		}
};
</script>

